{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "target": "ES5",
    "lib": ["ES2015"],
    "types": ["@types/google-apps-script"],
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "demo",
    "docs",
    ".git",
    "*.md"
  ]
}
